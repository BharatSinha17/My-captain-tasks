#include<stdio.h>
main()
{
	printf("MENU : \n1. Pizza-239\n2. Burger-129\n3. Pasta-179\n4. French Fries-99\n5. Sandwich-149 ");
	printf("\nEnter your order:");
	int choice=0;
	scanf("%d",&choice);
	
	switch(choice)
	
{
		case 1:
		printf("Pizza-239");
		break;
		
		case 2:
		printf("Burger-129");
		break;
			
		case 3:
		printf("Pasta-179");
		break;
		
		case 4:
		printf("French Fries-99");
		break;
		
		case 5:
		printf("Sandwich-149");
		break;
			
		default:
		printf("Re-enter your choice");
		}
    return 0;
}
